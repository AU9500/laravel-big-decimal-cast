{
  "name": "au9500/laravel-big-decimal-cast",
  "description": "Custom Brick/Math BigDecimal cast for Laravel Eloquent.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Au9500\\LaravelBigDecimalCast\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Au9500\\LaravelBigDecimalCast\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Andreas Uretschnig"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^8.2",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "brick/math": "^0.14",
    "illuminate/database": "^12.40"
  },
  "require-dev": {
    "laravel/sail": "^1.48",
    "laravel/pint": "^1.26",
    "phpunit/phpunit": "^12.4",
    "orchestra/testbench": "^10.8"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Au9500\\LaravelBigDecimalCast\\BigDecimalCastServiceProvider"
      ]
    }
  }
}
